defaultProxyProvider: default
docker:
  local:
    host: unix:///var/run/docker.sock
    targetHostname: {{ tsdproxy_target_hostname }}
    defaultProxyProvider: default
files: {}
tailscale:
  providers:
    default:
      authKey: "{{ tailscale_auth_key }}"
      controlUrl: {{ tsdproxy_control_url }}
  dataDir: {{ tsdproxy_data_dir }}
http:
  hostname: 0.0.0.0
  port: {{ tsdproxy_http_port }}
log:
  level: {{ tsdproxy_log_level }}
  json: {{ tsdproxy_log_json | lower }}
proxyAccessLog: {{ tsdproxy_proxy_access_log | lower }}
